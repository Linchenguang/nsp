          '_atom_site.aniso_U[1][1]'     alternate_exclusive 
          '_atom_site.aniso_B[1][1]' 
          '_atom_site_anisotrop.B[1][1]' alternate_exclusive 
          '_atom_site.aniso_B[1][1]' 
